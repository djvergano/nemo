{
  "scripts": {
    "i18n:export": "bundle exec rake i18n:js:export",
    "test": "jest",
    "lint:js": "eslint --ext .js,.jsx",
    "lint:react": "yarn run lint:js app/javascript/",
    "lint:backbone": "yarn run lint:js app/assets/javascripts/ --ignore-pattern app/assets/javascripts/legacy/",
    "lint:legacy": "yarn run lint:js app/assets/javascripts/legacy/",
    "lint:lib": "yarn run lint:js lib/assets/javascripts/",
    "lint:scss": "stylelint app/assets/stylesheets/"
  },
  "jest": {
    "roots": [
      "<rootDir>/app/javascript"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/javascript/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "/app/javascript/.*\\.test\\.jsx?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/app/javascript/__mocks__/style-mock.js"
    }
  },
  "resolutions_README": "Force babel-jest to use v23 as a workaround until we can upgrade to Babel 7. https://github.com/facebook/jest/issues/8230",
  "resolutions": {
    "babel-jest": "23.6.0"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@rails/webpacker": "^4.0.0",
    "ignore-loader": "0.1.2",
    "jquery": "3.4.1",
    "lodash": "4.17.13",
    "mobx": "5.9.4",
    "mobx-react": "5.4.3",
    "moment": "^2.24.0",
    "postcss-cssnext": "^3.1.0",
    "prop-types": "15.7.2",
    "query-string": "6.6.0",
    "react": "16.9.0",
    "react-bootstrap": "1.0.0-beta.9",
    "react-dates": "^21.0.0",
    "react-select2-wrapper": "sassafrastech/react-select2-wrapper#2019-03-08-select2Full",
    "react_ujs": "2.5.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-cooperka": "1.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "i18n-js": "3.3.0",
    "jest": "24.8.0",
    "react-dom": "16.9.0",
    "stylelint": "12.0.1",
    "stylelint-config-sass-guidelines": "6.2.0",
    "webpack-dev-server": "3.1.11"
  }
}
