{
  "scripts": {
    "i18n:export": "bundle exec rake i18n:js:export",
    "test": "jest",
    "lint:js": "eslint --ext .js,.jsx",
    "lint:react": "yarn run lint:js app/javascript/",
    "lint:backbone": "yarn run lint:js app/assets/javascripts/ --ignore-pattern app/assets/javascripts/legacy/",
    "lint:legacy": "yarn run lint:js app/assets/javascripts/legacy/",
    "lint:scss": "stylelint app/assets/stylesheets/"
  },
  "jest": {
    "roots": [
      "<rootDir>/app/javascript"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/javascript/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "/app/javascript/.*\\.test\\.jsx?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/app/javascript/__mocks__/style-mock.js"
    }
  },
  "dependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/runtime": "7.8.7",
    "@rails/webpacker": "4.2.2",
    "core-js": "3.6.4",
    "ignore-loader": "0.1.2",
    "jquery": "3.4.1",
    "lodash": "4.17.15",
    "mobx": "5.15.4",
    "mobx-react": "6.1.8",
    "moment": "2.24.0",
    "postcss-cssnext": "3.1.0",
    "prop-types": "15.7.2",
    "query-string": "6.11.1",
    "react": "16.13.0",
    "react-bootstrap": "1.0.0-beta.17",
    "react-dates": "21.8.0",
    "react-select2-wrapper": "sassafrastech/react-select2-wrapper#2019-03-08-select2Full",
    "react_ujs": "2.6.1",
    "regenerator-runtime": "0.13.5"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "caniuse-lite": "1.0.30001035",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-cooperka": "1.0.4",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "i18n-js": "3.5.1",
    "jest": "25.1.0",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "6.7.0",
    "react-dom": "16.13.0",
    "react-with-direction": "1.3.1",
    "stylelint": "13.2.1",
    "stylelint-config-sass-guidelines": "7.0.0",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
