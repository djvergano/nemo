<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="demo" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <ComplexType Name="response_meta">
        <Property Name="shortcode" Type="Edm.String"/>
        <Property Name="form_name" Type="Edm.String"/>
        <Property Name="submitter_name" Type="Edm.String"/>
        <Property Name="submission_time" Type="Edm.DateTimeOffset"/>
        <Property Name="reviewed" Type="Edm.Boolean"/>
      </ComplexType>
      <EntityType Name="response:form_a">
        <Key>
          <PropertyRef Name="_id"/>
        </Key>
        <Property Name="_id" Type="Edm.String"/>
        <Property Name="_meta" Type="demo.response_meta"/>
        <Property Name="name" Type="Edm.String"/>
        <Property Name="age" Type="Edm.Int32"/>
      </EntityType>
      <EntityType Name="response:form_b">
        <Key>
          <PropertyRef Name="_id"/>
        </Key>
        <Property Name="_id" Type="Edm.String"/>
        <Property Name="_meta" Type="demo.response_meta"/>
        <Property Name="Household members" Type="Collection(demo.repeat:form_b:r1)"/>
      </EntityType>
      <EntityType Name="repeat:form_b:r1">
        <Property Name="name" Type="Edm.String"/>
        <Property Name="age" Type="Edm.Int32"/>
        <Property Name="eyes" Type="Collection(demo.repeat:form_b:r2)"/>
      </EntityType>
      <EntityType Name="repeat:form_b:r2">
        <Property Name="side" Type="Edm.String"/>
        <Property Name="healthy" Type="Edm.Boolean"/>
      </EntityType>
      <EntityType Name="user">
        <Key>
          <PropertyRef Name="id"/>
        </Key>
        <Property Name="id" Type="Edm.String"/>
        <Property Name="name" Type="Edm.String"/>
      </EntityType>
      <EntityContainer Name="DefaultContainer">
        <EntitySet Name="responses:form_a" EntityType="demo.response:form_a">
          <Annotation Term="Org.OData.Core.V1.ResourcePath" String="responses_form_a"/>
        </EntitySet>
        <EntitySet Name="responses:form_b" EntityType="demo.response:form_b">
          <Annotation Term="Org.OData.Core.V1.ResourcePath" String="responses_form_b"/>
        </EntitySet>
      </EntityContainer>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
